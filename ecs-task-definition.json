{
  "family": "my-github-app-task-definition",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::149831571207:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "my-github-app-task-definition",
      "image": "149831571207.dkr.ecr.ap-south-1.amazonaws.com/fastapi-github-actions:latest",
      "portMappings": [
        {
          "containerPort": 8001,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
